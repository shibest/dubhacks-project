{
  "buildCommand": "npm run start",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "backend/server.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "VITE_APP_URL": "https://myceli.us",
    "VITE_BACKEND_URL": "https://myceli.us/api"
  }
}
